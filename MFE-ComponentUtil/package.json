{
  "name": "@org/MFE-ComponentUil",
  "scripts": {
    "start": "webpack serve --port 3004",
    "start:standalone": "webpack serve --env standalone",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint --format json --output-file lint/ESOutput.json .; exit 0",
    "lint:local": "eslint .; exit 0",
    "format": "prettier --write './**'",
    "test": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=reporter-config.json",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cy:verify": "cypress verify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently npm:test npm:lint"
    }
  },
